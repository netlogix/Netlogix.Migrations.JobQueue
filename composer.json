{
    "name": "netlogix/migrations-jobqueue",
    "description": "Run netlogix/migrations asynchronously using flowpack/jobqueue",
    "homepage": "http://websolutions.netlogix.de/",
    "license": ["MIT"],
    "type": "neos-package",
    "require": {
        "php": "^7.4 || ^8.2",
        "neos/flow": "^7.3 || ^8.3",
        "netlogix/migrations": "^2.0",
        "flowpack/jobqueue-common": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Netlogix\\Migrations\\JobQueue\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netlogix\\Migrations\\JobQueue\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Netlogix.Migrations.JobQueue"
        }
    }
}
